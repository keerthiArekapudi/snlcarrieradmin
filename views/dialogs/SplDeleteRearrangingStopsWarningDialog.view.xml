<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc"
	xmlns:layout="sap.ui.layout" xmlns="sap.m"
	controllerName="splController.dialogs.SplDeleteRearrangingStopsWarningDialog"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<layout:VerticalLayout  width="100%">
		<layout:content>
		
			<Text text = "{/DialogText1}">
			</Text>
			<Text text = "{/DialogText2}" class="SapSplAdditionalText">
			</Text>
			
			<layout:VerticalLayout  width="100%" content="{/AffectedFreightItems}">
				<layout:content>
					<Label design = "Bold" text = "{ItemID}">
					</Label>
				</layout:content>
			</layout:VerticalLayout>
		</layout:content>
	</layout:VerticalLayout>
</core:View>